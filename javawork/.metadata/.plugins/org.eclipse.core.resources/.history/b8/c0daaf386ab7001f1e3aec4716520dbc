package day1211;

public class Ex1DataType {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		byte a=127;
		byte b=(byte)128;//(byte):강제 형변환 - cast 연산
		byte c=(byte)200;
		
		System.out.println("a="+a);//127범위안에들어
		System.out.println("b="+b);//-128:틀린값이나옴, 범위를벗어나므로
		System.out.println("c="+c);

	}

}
