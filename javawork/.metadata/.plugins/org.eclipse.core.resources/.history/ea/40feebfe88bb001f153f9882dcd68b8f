package day1216;

public class Ex10LoopExam {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int i=1;i<=1;i++)//행에 대한 반복
		{
			for(int j=1;j<=9;j++)//열에 대한 반복
			{
				System.out.print("["+j+"단]\t");
			}
			System.out.println();
		}
		System.out.println("=".repeat(75));
		for(int j=1;j<=9;j++)//열에 대한 반복
		{
			System.out.print("["+j+"단]\t");
		}
		System.out.println();

	}

}
